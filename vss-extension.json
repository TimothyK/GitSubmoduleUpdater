{
  "manifestVersion": 1,
  "id": "git-submodule-updater",
  "name": "Git Submodule Updater",
  "version": "0.0.0",
  "publisher": "CodeMonkeyProjectiles",
  "description": "Azure DevOps pipeline task to check git submodules and determine which ones need updating to the latest commits",
  "public": true,
  "categories": [
    "Azure Pipelines"
  ],
  "tags": [
    "Git",
    "Submodule",
    "CI/CD",
    "Pipeline",
    "Version Control",
    "Build",
    "DevOps"
  ],
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],

  "icons": {
    "default": "assets/logo.png",
    "large": "assets/logo.png"
  },
  "screenshots": [
    {
      "path": "assets/screenshot-output.png"
    }
  ],
  "content": {
    "details": {
      "path": "overview.md"
    },
    "license": {
      "path": "LICENSE"
    }
  },
  "links": {
    "home": {
      "uri": "https://github.com/TimothyK/GitSubmoduleUpdater"
    },
    "getstarted": {
      "uri": "https://github.com/TimothyK/GitSubmoduleUpdater#usage"
    },
    "repository": {
      "uri": "https://github.com/TimothyK/GitSubmoduleUpdater"
    },
    "support": {
      "uri": "https://github.com/TimothyK/GitSubmoduleUpdater/issues"
    }
  },
  "repository": {
    "type": "git",
    "uri": "https://github.com/TimothyK/GitSubmoduleUpdater"
  },
  "branding": {
    "color": "rgb(34, 139, 34)",
    "theme": "dark"
  },
  "contributions": [
    {
      "id": "git-submodule-updater-task",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "GitSubmoduleUpdater"
      }
    }
  ],
  "files": [
    {
      "path": "task.json",
      "addressable": true,
      "packagePath": "GitSubmoduleUpdater/task.json"
    },
    {
      "path": "icon.png",
      "addressable": true,
      "packagePath": "GitSubmoduleUpdater/icon.png"
    },
    {
      "path": "dist",
      "addressable": true,
      "packagePath": "GitSubmoduleUpdater/dist"
    },
    {
      "path": "node_modules/azure-pipelines-task-lib",
      "addressable": true,
      "packagePath": "GitSubmoduleUpdater/node_modules/azure-pipelines-task-lib"
    },
    {
      "path": "node_modules/simple-git",
      "addressable": true,
      "packagePath": "GitSubmoduleUpdater/node_modules/simple-git"
    },
    {
      "path": "node_modules/concat-map",
      "addressable": true,
      "packagePath": "GitSubmoduleUpdater/node_modules/concat-map"
    },
    {
      "path": "node_modules/balanced-match",
      "addressable": true,
      "packagePath": "GitSubmoduleUpdater/node_modules/balanced-match"
    },
    {
      "path": "node_modules/brace-expansion",
      "addressable": true,
      "packagePath": "GitSubmoduleUpdater/node_modules/brace-expansion"
    },
    {
      "path": "node_modules/uuid",
      "addressable": true,
      "packagePath": "GitSubmoduleUpdater/node_modules/uuid"
    },
    {
      "path": "node_modules/underscore",
      "addressable": true,
      "packagePath": "GitSubmoduleUpdater/node_modules/underscore"
    },
    {
      "path": "node_modules/mkdirp",
      "addressable": true,
      "packagePath": "GitSubmoduleUpdater/node_modules/mkdirp"
    },
    {
      "path": "node_modules/adm-zip",
      "addressable": true,
      "packagePath": "GitSubmoduleUpdater/node_modules/adm-zip"
    },
    {
      "path": "node_modules/q",
      "addressable": true,
      "packagePath": "GitSubmoduleUpdater/node_modules/q"
    },
    {
      "path": "node_modules/shelljs",
      "addressable": true,
      "packagePath": "GitSubmoduleUpdater/node_modules/shelljs"
    },
    {
      "path": "node_modules/nodejs-file-downloader",
      "addressable": true,
      "packagePath": "GitSubmoduleUpdater/node_modules/nodejs-file-downloader"
    },
    {
      "path": "node_modules/@kwsites",
      "addressable": true,
      "packagePath": "GitSubmoduleUpdater/node_modules/@kwsites"
    },
    {
      "path": "node_modules/debug",
      "addressable": true,
      "packagePath": "GitSubmoduleUpdater/node_modules/debug"
    },
    {
      "path": "node_modules/ms",
      "addressable": true,
      "packagePath": "GitSubmoduleUpdater/node_modules/ms"
    },
    {
      "path": "assets",
      "addressable": true,
      "packagePath": "assets"
    }
  ]
}